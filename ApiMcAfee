          name: telefono
          description: telefono
          schema:
            type: integer
        - in: query
          name: cliente
          description: cliente
          schema:
            type: string
        - in: query
          name: folioContratacion
          description: folio de contratacion
          schema:
            type: integer
            format: int32
            minimum: 0
            maximum: 50
        - in: query
          name: accion
          description: accion
          schema:
            type: string
        - in: query
          name: producto
          description: producto
          schema:
            type: string
        - in: query
          name: folioG3
          description: folioG3
          schema:
            type: string
