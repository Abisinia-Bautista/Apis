components:
  schemas:
    InventoryItem:
      type: object
      required:
        - category
        - description
        - feature
        - relatedParty
        - serviceCharacteristic
        - serviceSpecification
        - state
        - supportingService
        - type
      properties:
        category:
          type: string
          example: MobileService
        description:
          type: string
          example: Mobile Line
        feature:
          $ref: '#/components/schemas/feature'
        relatedParty:
          $ref: '#/components/schemas/relatedParty'
        serviceCharacteristic:
          $ref: '#/components/schemas/serviceCharacteristic'
        serviceSpecification:
          $ref: '#/components/schemas/serviceSpecification'
        state:
          type: string
          example: active
        supportingService:
          $ref: '#/components/schemas/supportingService'
        type:
          type: string
          example: Service
    feature: 
      type: object
      required: 
        - id
    relatedParty:
      type: object
      required: 
        - id
    serviceCharacteristic:
      type: array
      required:
        - id
        - name
        - valueType
        - value
      items:
        properties:
          id: 
            type: integer
            example: 90
          name:
            type: string
            example: canal
          valueType:
            type: string
            example: string
          value:
            type: string
            example: TMXCO
    serviceSpecification:
      type: object
      required: 
        - id
    supportingService:
      type: object
      required: 
        - id 

    RespuestaExito:
      type: array
      items:
        properties:
          canal:
            type: string
            example: TMXCO
            description: canal que pertenece
          usuario:
            type: string
            example: 9999999
            description: usuario
          oficina:
            type: string
            example: TMX
            description: oficina
